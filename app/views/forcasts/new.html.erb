<%= form_tag(forcasts_path, method: :post) do %>
   <%= label_tag(:address, "Address:") %>
   <%= text_field_tag(:address) %></br></br>
   <%= label_tag(:city, "City:") %>
   <%= text_field_tag(:city) %></br></br>
   <%= label_tag(:state, "State:") %>
   <%= text_field_tag(:state) %></br></br>
   <%= label_tag(:country, "Country:") %>
   <%= text_field_tag(:country) %></br></br>
   <%= label_tag(:zip_code, "Zip Code:") %>
   <%= text_field_tag(:zip_code) %></br></br>
   <%= submit_tag("Search") %>
<% end %>